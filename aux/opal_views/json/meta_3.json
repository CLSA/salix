          {
            "name": "script",
            "value": "// the user who ran the onyx bone density qnaire stage just before\n// performing the dexa exam\n//\nvar q=$('clsa-dcs-f2.QuestionnaireBoneDensity:QuestionnaireRun.user').lowerCase()\n\n// the user who ran the dexa image transfer to onyx, who may or may not\n// be the same user who actually performed the exam\n//\nvar wu=$('clsa-dcs-images-f2.WholeBodyBoneDensity:InstrumentRun.user').lowerCase()\nvar lu=$('clsa-dcs-images-f2.LateralBoneDensity:InstrumentRun.user').lowerCase()\nvar hu=$('clsa-dcs-images-f2.DualHipBoneDensity:InstrumentRun.user').lowerCase()\nvar fu=$('clsa-dcs-images-f2.ForearmBoneDensity:InstrumentRun.user').lowerCase()\nvar su=$('clsa-dcs-images-f2.SpineBoneDensity:InstrumentRun.user').lowerCase()\nvar a = new Array()\nif(wu.isNull().not().value() && a.indexOf(wu.value())<0) a.push(wu.value())\nif(lu.isNull().not().value() && a.indexOf(lu.value())<0) a.push(lu.value())\nif(hu.isNull().not().value() && a.indexOf(hu.value())<0) a.push(hu.value())\nif(fu.isNull().not().value() && a.indexOf(fu.value())<0) a.push(fu.value())\nif(su.isNull().not().value() && a.indexOf(su.value())<0) a.push(su.value())\nvar res=null\nif(0<a.length) {\n  res=newValue(a.join('/'))\n  if(q.isNull().not().value() && a.indexOf(q.value())<0) {\n    res=res.concat('/').concat(q)\n  }\n}\nres"
          }
        ],
        "entityType": "Participant",
        "index": 0,
        "isRepeatable": false,
        "mimeType": "",
        "name": "technician",
        "occurrenceGroup": "",
        "referencedEntityType": "",
        "unit": "",
        "valueType": "text"
